{
  "name": "hapi-socket-react-boilerplate",
  "version": "1.4.0",
  "description": "Boilerplate app to get you up and running with Hapi, HTTPS, HTTP/2, Web Socket and React/Redux.",
  "main": "src/server/index.js",
  "homepage": "https://hapi-boilerplate-docker.herokuapp.com/index",
  "repository": {
    "type": "git",
    "url": "https://github.com/blairg/hapi-socket-react-boilerplate.git"
  },
  "bugs": {
    "url": "https://github.com/blairg/hapi-socket-react-boilerplate/issues",
    "email": "blair.garrett1@gmail.com"
  },
  "keywords": [
    "hapi.js",
    "http2",
    "react",
    "redux",
    "webpack",
    "socket.io",
    "jest"
  ],
  "scripts": {
    "build:client": "webpack --progress --colors --verbose --config webpack.client.config",
    "build:client:watch": "webpack --hot --watch --progress --colors --verbose --config webpack.client.config",
    "build:client:prod": "NODE_ENV=production webpack -p --progress --colors --config webpack.client.prod.config",
    "build:dev": "rm -rf ./dist && npm run build:client && npm run build:server",
    "build:server": "webpack --progress --colors --verbose --config webpack.server.config",
    "build:server:watch": "webpack --hot --watch --progress --colors --verbose --config webpack.server.config",
    "build:server:prod": "NODE_ENV=production webpack -p --progress --colors --config webpack.server.config",
    "build:watch": "webpack --progress --colors --watch --verbose ",
    "build:prod": "rm -rf ./dist && npm run build:client:prod && npm run build:server:prod",
    "createCertificate": "sudo chmod +x ./ssl/createCerts.sh && sh ./ssl/createCerts.sh",
    "dev": "rm -rf ./dist && npm-run-all --parallel build:client:watch build:server:watch dev:server",
    "dev:server": "npm run build:server && NODE_NO_HTTP2=1 nodemon ./dist/server.js",
    "dev:debug": "NODE_NO_HTTP2=1 nodemon --nolazy --inspect-brk=5858 -w src --exec \"babel-node src/server/index.js --presets es2015,stage-0,react\"",
    "lint": "./node_modules/.bin/eslint src/ --fix && sass-lint -c .scss-lint.yml 'src/stylesheets/**/*.scss' -v -q",
    "lint:scss": "sass-lint -c .scss-lint.yml 'src/stylesheets/**/*.scss' -v -q",
    "pretty": "prettier --single-quote --trailing-comma all --write \"{src,test}/**/**/*.js*\" \"{src,test}/**/**/**/*.js*\"",
    "start": "NODE_NO_HTTP2=1 node ./dist/server.js",
    "test": "jest --config jest.config.js",
    "test:coverage": "jest --config jest.config.js --coverage",
    "test:update": "jest --updateSnapshot",
    "test:watch": "jest --watch",
    "storybook": "start-storybook -p 6006 -c .storybook",
    "storybook:build": "build-storybook"
  },
  "author": "Blair Garrett <blair.garrett1@gmail.com>",
  "license": "ISC",
  "devDependencies": {
    "@storybook/addon-actions": "^3.3.3",
    "@storybook/addon-links": "^3.3.3",
    "@storybook/react": "^3.3.3",
    "autoprefixer": "^7.2.3",
    "axios-mock-adapter": "^1.10.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.1.2",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "babel-runtime": "^6.26.0",
    "babel-watch": "^2.0.7",
    "coveralls": "^3.0.0",
    "cssnano": "^3.10.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.14.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.5.1",
    "eventsource-polyfill": "^0.9.6",
    "extract-text-webpack-plugin": "^3.0.2",
    "jest": "^22.0.4",
    "jest-cli": "^22.0.4",
    "jsdom": "^11.5.1",
    "jsdom-global": "3.0.2",
    "node-sass": "^4.7.2",
    "nodemon": "^1.14.3",
    "npm-run-all": "^4.1.2",
    "postcss": "^6.0.14",
    "postcss-cssnext": "^3.0.2",
    "postcss-import": "^11.0.0",
    "postcss-loader": "^2.0.9",
    "postcss-scss": "^1.0.2",
    "prettier": "^1.9.2",
    "purify-css": "^1.2.5",
    "purifycss-webpack": "^0.7.0",
    "react-test-renderer": "^16.2.0",
    "redux-mock-store": "^1.4.0",
    "sass-lint": "^1.12.1",
    "sass-loader": "^6.0.6",
    "sugarss": "^1.0.1",
    "webpack": "^3.10.0",
    "webpack-merge": "^4.1.1",
    "webpack-node-externals": "^1.6.0"
  },
  "dependencies": {
    "axios": "^0.17.1",
    "decode-html": "^2.0.0",
    "hapi": "16.2.0",
    "hapi-swagger": "^8.0.0",
    "http-status": "^1.0.1",
    "http2": "^3.3.7",
    "inert": "^4.2.1",
    "memory-cache": "^0.2.0",
    "moment": "^2.20.1",
    "nes": "^6.5.2",
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.6",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0",
    "vision": "^4.1.1",
    "winston": "^2.4.0"
  },
  "peerDependencies": {
    "babel-core": "^6.26.0"
  },
  "engines": {
    "node": "8.9.3",
    "npm": "5.6.0"
  }
}
