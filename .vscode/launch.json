    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Debug Dev",
            "runtimeExecutable": "npm",
            "runtimeArgs": [
                "run-script",
                "dev:debug"
            ],
            "port": 5858,
            "skipFiles": [
                "<node_internals>/**/*.js",
                "${workspaceFolder}/node_modules/**/*.js"
            ]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Debug Dev with Modules",
            "runtimeExecutable": "npm",
            "runtimeArgs": [
                "run-script",
                "dev:debug"
            ],
            "port": 5858
        }
    ]
}